<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<?eclipse version="3.4"?>
<plugin>

    <extension id="OAFView"
               point="org.protege.editor.core.application.ViewComponent">
        
        <label value="OAF for Protege"/>
        <class value="edu.njit.cs.saboc.blu.owl.protege.OAFAbNProtegePlugin"/>
        <headerColor value="@org.protege.ontologycolor"/>
        <category value="@org.protege.ontologycategory"/>
    </extension>
    
    <extension id="OAFTab"
               point="org.protege.editor.core.application.WorkspaceTab">
        
        <label value="OAF for Protege"/>
        <class value="org.protege.editor.owl.ui.OWLWorkspaceViewsTab"/>
        <index value="X"/>
      
        <editorKitId value="OWLEditorKit"/>
      
        <defaultViewConfigFileName value="OAFTabLayout.xml"/>
    </extension>
   
    <extension id="OAFNATView" point="org.protege.editor.core.application.ViewComponent">
        
        <label value="OAF Class Browser"/>
        <class value="edu.njit.cs.saboc.blu.owl.protege.OAFProtegeNAT"/>
        
        <headerColor value="@org.protege.ontologycolor"/>
        <category value="@org.protege.ontologycategory"/>
        
    </extension>
    
    <extension id="NATTab" 
               point="org.protege.editor.core.application.WorkspaceTab">
        
        <label value="OAF Class Browser"/>
        <class value="org.protege.editor.owl.ui.OWLWorkspaceViewsTab"/>
        <index value="X"/>
      
        <editorKitId value="OWLEditorKit"/>
      
        <defaultViewConfigFileName value="OAFNATLayout.xml"/>
    </extension>
   
    <extension id="OAFLiveDiffView"
               point="org.protege.editor.core.application.ViewComponent">
        
        <label value="OAF Live Diff Taxonomies"/>
        <class value="edu.njit.cs.saboc.blu.owl.protege.LiveTaxonomyView"/>
        <headerColor value="@org.protege.ontologycolor"/>
        <category value="@org.protege.ontologycategory"/>
        
    </extension>
   
</plugin>
